{"ast":null,"code":"import \"../Firebase\";\nimport { collection, query, getFirestore, onSnapshot, doc, deleteDoc } from \"firebase/firestore\";\nimport printJS from \"print-js\";\nconst db = getFirestore();\nconst q = query(collection(db, \"Checkout\"));\nexport default {\n  name: \"OrdersView\",\n\n  data() {\n    return {\n      orders: [],\n      dataProduct: {\n        name: \"\",\n        price: 0,\n        image: \"\",\n        brand: \"\",\n        description: \"\",\n        key: \"\"\n      },\n      productdata: {\n        address: \"\",\n        date: \"\",\n        name: \"\",\n        number: \"\",\n        total: \"\",\n        cart: [],\n        key: \"\"\n      }\n    };\n  },\n\n  created() {\n    onSnapshot(q, querySnapshot => {\n      this.orders = [];\n      querySnapshot.forEach(doc => {\n        this.orders.push({\n          key: doc.id,\n          name: doc.data().name,\n          address: doc.data().address,\n          number: doc.data().number,\n          date: doc.data().date,\n          total: doc.data().total,\n          cart: doc.data().cart\n        });\n      });\n    });\n  },\n\n  methods: {\n    getDataKey(key, name) {\n      this.dataProduct.key = key;\n      this.dataProduct.name = name;\n    },\n\n    RemoveFromCheckout(key) {\n      deleteDoc(doc(db, \"Checkout\", key)).then(() => {\n        document.getElementById(\"close\").click();\n      });\n    },\n\n    GetOrderData(name, date, address, number, cart, key, total) {\n      this.productdata.name = name;\n      this.productdata.date = date;\n      this.productdata.address = address;\n      this.productdata.number = number;\n      this.productdata.cart = cart;\n      this.productdata.key = key;\n      this.productdata.total = total;\n      console.log(this.productdata);\n    },\n\n    printinvoice() {\n      printJS({\n        printable: \"printJS-form\",\n        type: \"html\",\n        targetStyles: [\"*\"],\n        scanStyles: true\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyUA,OAAO,aAAP;AAEA,SACEA,UADF,EAEEC,KAFF,EAGEC,YAHF,EAIEC,UAJF,EAKEC,GALF,EAMEC,SANF,QAOO,oBAPP;AAQA,OAAOC,OAAP,MAAoB,UAApB;AACA,MAAMC,EAAC,GAAIL,YAAY,EAAvB;AACA,MAAMM,IAAIP,KAAK,CAACD,UAAU,CAACO,EAAD,EAAK,UAAL,CAAX,CAAf;AAEA,eAAe;EACbE,IAAI,EAAE,YADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,MAAM,EAAE,EADH;MAELC,WAAW,EAAE;QACXH,IAAI,EAAE,EADK;QAEXI,KAAK,EAAE,CAFI;QAGXC,KAAK,EAAE,EAHI;QAIXC,KAAK,EAAE,EAJI;QAKXC,WAAW,EAAE,EALF;QAMXC,GAAG,EAAE;MANM,CAFR;MAULC,WAAW,EAAE;QACXC,OAAO,EAAE,EADE;QAEXC,IAAI,EAAE,EAFK;QAGXX,IAAI,EAAE,EAHK;QAIXY,MAAM,EAAE,EAJG;QAKXC,KAAK,EAAE,EALI;QAMXC,IAAI,EAAE,EANK;QAOXN,GAAG,EAAE;MAPM;IAVR,CAAP;EAoBD,CAvBY;;EAwBbO,OAAO,GAAG;IACRrB,UAAU,CAACK,CAAD,EAAKiB,aAAD,IAAmB;MAC/B,KAAKd,MAAL,GAAc,EAAd;MACAc,aAAa,CAACC,OAAd,CAAuBtB,GAAD,IAAS;QAC7B,KAAKO,MAAL,CAAYgB,IAAZ,CAAiB;UACfV,GAAG,EAAEb,GAAG,CAACwB,EADM;UAEfnB,IAAI,EAAEL,GAAG,CAACM,IAAJ,GAAWD,IAFF;UAGfU,OAAO,EAAEf,GAAG,CAACM,IAAJ,GAAWS,OAHL;UAIfE,MAAM,EAAEjB,GAAG,CAACM,IAAJ,GAAWW,MAJJ;UAKfD,IAAI,EAAEhB,GAAG,CAACM,IAAJ,GAAWU,IALF;UAMfE,KAAK,EAAElB,GAAG,CAACM,IAAJ,GAAWY,KANH;UAOfC,IAAI,EAAEnB,GAAG,CAACM,IAAJ,GAAWa;QAPF,CAAjB;MASD,CAVD;IAWD,CAbS,CAAV;EAcD,CAvCY;;EAwCbM,OAAO,EAAE;IACPC,UAAU,CAACb,GAAD,EAAMR,IAAN,EAAY;MACpB,KAAKG,WAAL,CAAiBK,GAAjB,GAAuBA,GAAvB;MACA,KAAKL,WAAL,CAAiBH,IAAjB,GAAwBA,IAAxB;IACD,CAJM;;IAMPsB,kBAAkB,CAACd,GAAD,EAAM;MACtBZ,SAAS,CAACD,GAAG,CAACG,EAAD,EAAK,UAAL,EAAiBU,GAAjB,CAAJ,CAAT,CAAoCe,IAApC,CAAyC,MAAM;QAC7CC,QAAQ,CAACC,cAAT,CAAwB,OAAxB,EAAiCC,KAAjC;MACD,CAFD;IAGD,CAVM;;IAWPC,YAAY,CAAC3B,IAAD,EAAOW,IAAP,EAAaD,OAAb,EAAsBE,MAAtB,EAA8BE,IAA9B,EAAoCN,GAApC,EAAyCK,KAAzC,EAAgD;MAC1D,KAAKJ,WAAL,CAAiBT,IAAjB,GAAwBA,IAAxB;MACA,KAAKS,WAAL,CAAiBE,IAAjB,GAAwBA,IAAxB;MACA,KAAKF,WAAL,CAAiBC,OAAjB,GAA2BA,OAA3B;MACA,KAAKD,WAAL,CAAiBG,MAAjB,GAA0BA,MAA1B;MACA,KAAKH,WAAL,CAAiBK,IAAjB,GAAwBA,IAAxB;MACA,KAAKL,WAAL,CAAiBD,GAAjB,GAAuBA,GAAvB;MACA,KAAKC,WAAL,CAAiBI,KAAjB,GAAyBA,KAAzB;MACAe,OAAO,CAACC,GAAR,CAAY,KAAKpB,WAAjB;IACD,CApBM;;IAqBPqB,YAAY,GAAG;MACbjC,OAAO,CAAC;QACNkC,SAAS,EAAE,cADL;QAENC,IAAI,EAAE,MAFA;QAGNC,YAAY,EAAE,CAAC,GAAD,CAHR;QAINC,UAAU,EAAE;MAJN,CAAD,CAAP;IAMD;;EA5BM;AAxCI,CAAf","names":["collection","query","getFirestore","onSnapshot","doc","deleteDoc","printJS","db","q","name","data","orders","dataProduct","price","image","brand","description","key","productdata","address","date","number","total","cart","created","querySnapshot","forEach","push","id","methods","getDataKey","RemoveFromCheckout","then","document","getElementById","click","GetOrderData","console","log","printinvoice","printable","type","targetStyles","scanStyles"],"sourceRoot":"","sources":["C:\\Users\\muham\\OneDrive\\Masaüstü\\project\\Online-Store\\src\\views\\Orders.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <br />\r\n    <h1 style=\"text-align: center\">Orders</h1>\r\n    <br />\r\n    <table class=\"table table-bordered\" v-if=\"orders.length > 0\">\r\n      <thead style=\"background: #39b5ff; color: white\">\r\n        <tr>\r\n          <!-- <th scope=\"col\"></th> -->\r\n          <th scope=\"col\">name</th>\r\n          <th scope=\"col\">Date</th>\r\n          <th scope=\"col\">Address</th>\r\n          <th scope=\"col\">Number</th>\r\n          <th scope=\"col\">Products</th>\r\n          <th scope=\"col\">Actions</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"item in orders\" :key=\"item.name\">\r\n          <!-- <th scope=\"row\"></th> -->\r\n          <td class=\"align-middle\">{{ item.name }}</td>\r\n          <td class=\"align-middle\">{{ item.date }}</td>\r\n          <td class=\"align-middle\">{{ item.address }}</td>\r\n          <td class=\"align-middle\">{{ item.number }}</td>\r\n          <td>\r\n            <table class=\"table mb-0 table-bordered\">\r\n              <thead>\r\n                <tr class=\"table-secondary\">\r\n                  <th scope=\"col\">Name</th>\r\n                  <th scope=\"col\">Quantity</th>\r\n                  <th scope=\"col\">Price</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                <tr v-for=\"cartitem in item.cart\" :key=\"cartitem.name\">\r\n                  <td>{{ cartitem.name }}</td>\r\n                  <td>{{ cartitem.quantity }}</td>\r\n                  <td>{{ cartitem.totalPrice }} DT</td>\r\n                </tr>\r\n                <tr>\r\n                  <td colspan=\"2\" class=\"table-active\">\r\n                    Total Price (shipping included)\r\n                  </td>\r\n                  <td>{{ item.total }} DT</td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </td>\r\n          <td>\r\n            <div class=\"d-grid gap-2\">\r\n              <button\r\n                type=\"button\"\r\n                @click=\"\r\n                  GetOrderData(\r\n                    item.name,\r\n                    item.date,\r\n                    item.address,\r\n                    item.number,\r\n                    item.cart,\r\n                    item.key,\r\n                    item.total\r\n                  )\r\n                \"\r\n                class=\"btn btn-success\"\r\n                data-bs-toggle=\"modal\"\r\n                data-bs-target=\"#validation\"\r\n              >\r\n                Validation\r\n              </button>\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn\"\r\n                style=\"background: red; color: white\"\r\n                data-bs-toggle=\"modal\"\r\n                data-bs-target=\"#delete\"\r\n                v-on:click=\"getDataKey(item.key, item.name)\"\r\n              >\r\n                Delete\r\n              </button>\r\n            </div>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n    <div\r\n      v-else\r\n      class=\"row d-flex justify-content-center align-items-center h-100\"\r\n    >\r\n      <br />\r\n      <div>\r\n        <center>orders page is empty</center>\r\n        <br />\r\n        <br />\r\n        <router-link to=\"/products\">\r\n          <img src=\"../assets/icons8_back_64px.png\" style=\"height: 37px\" />\r\n        </router-link>\r\n        <a style=\"width: auto\"> Continue shopping </a>\r\n      </div>\r\n    </div>\r\n    <!-- Modal 3 (delete) -->\r\n    <div\r\n      class=\"modal fade\"\r\n      id=\"delete\"\r\n      data-bs-backdrop=\"static\"\r\n      data-bs-keyboard=\"false\"\r\n      tabindex=\"-1\"\r\n      aria-labelledby=\"staticBackdropLabel\"\r\n      aria-hidden=\"true\"\r\n    >\r\n      <div class=\"modal-dialog\">\r\n        <div class=\"modal-content\">\r\n          <div class=\"modal-header\">\r\n            <h5 class=\"modal-title\" id=\"staticBackdropLabel\">Delete order</h5>\r\n            <button\r\n              type=\"button\"\r\n              class=\"btn-close\"\r\n              data-bs-dismiss=\"modal\"\r\n              aria-label=\"Close\"\r\n            ></button>\r\n          </div>\r\n          <div class=\"modal-body\">\r\n            .\r\n            <!-- form start here -->\r\n            <form>\r\n              <h3>\r\n                Do you want to delete this order of {{ dataProduct.name }} ?\r\n              </h3>\r\n            </form>\r\n            <!-- form end here -->\r\n          </div>\r\n          <div class=\"modal-footer\">\r\n            <button\r\n              type=\"button\"\r\n              id=\"close\"\r\n              class=\"btn btn-secondary\"\r\n              data-bs-dismiss=\"modal\"\r\n            >\r\n              Close\r\n            </button>\r\n            <button\r\n              type=\"button\"\r\n              class=\"btn\"\r\n              style=\"background: red; color: white\"\r\n              v-on:click=\"RemoveFromCheckout(dataProduct.key)\"\r\n            >\r\n              Delete\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- fin model -->\r\n    <!-- Modal  (validation) -->\r\n    <div\r\n      class=\"modal fade\"\r\n      id=\"validation\"\r\n      data-bs-backdrop=\"static\"\r\n      data-bs-keyboard=\"false\"\r\n      tabindex=\"-1\"\r\n      aria-labelledby=\"staticBackdropLabel\"\r\n      aria-hidden=\"true\"\r\n    >\r\n      <div class=\"modal-dialog modal-xl\">\r\n        <div class=\"modal-content\" style=\"border: black\">\r\n          <div class=\"modal-header\" style=\"background: #242121; color: white\">\r\n            <h5\r\n              class=\"modal-title\"\r\n              id=\"staticBackdropLabel\"\r\n              style=\"display: flex; margin: auto\"\r\n            >\r\n              Print the invoice\r\n            </h5>\r\n          </div>\r\n          <div class=\"modal-body\">\r\n            <!-- form start here -->\r\n            <div class=\"container\" id=\"printJS-form\">\r\n              <div class=\"col-md-12\">\r\n                <div class=\"invoice\">\r\n                  <!-- begin invoice-company -->\r\n\r\n                  <div class=\"row align-items-start\">\r\n                    <div class=\"col\">SelDig, Inc</div>\r\n                  </div>\r\n                  <!-- end invoice-company -->\r\n                  <!-- begin invoice-header -->\r\n                  <div class=\"invoice-header\">\r\n                    <div class=\"invoice-from\">\r\n                      <small>from</small>\r\n                      <address class=\"m-t-5 m-b-5\">\r\n                        <strong class=\"text-inverse\">SelDig, Inc.</strong><br />\r\n                        Address: Ouerghech, Ghardimaou<br />\r\n                        Phone: (+216) 54 819 819\r\n                      </address>\r\n                    </div>\r\n                    <div class=\"invoice-to\">\r\n                      <small>to</small>\r\n                      <address class=\"m-t-5 m-b-5\">\r\n                        <strong class=\"text-inverse\">{{\r\n                          productdata.name\r\n                        }}</strong\r\n                        ><br />\r\n                        Address: {{ productdata.address }}<br />\r\n                        Phone: (+216) {{ productdata.number }}\r\n                      </address>\r\n                    </div>\r\n                    <div class=\"invoice-date\">\r\n                      <small>Invoice / July period</small>\r\n                      <div class=\"date text-inverse m-t-5\">\r\n                        {{ productdata.date }}\r\n                      </div>\r\n                      <br />\r\n                      <div class=\"invoice-detail\">Services Product</div>\r\n                    </div>\r\n                  </div>\r\n                  <!-- end invoice-header -->\r\n                  <!-- begin invoice-content -->\r\n                  <div class=\"invoice-content\">\r\n                    <!-- begin table-responsive -->\r\n                    <div class=\"table-responsive\">\r\n                      <table class=\"table table-invoice\">\r\n                        <thead>\r\n                          <tr>\r\n                            <th>PRODUCTS</th>\r\n                            <th class=\"text-center\" width=\"10%\">RATE</th>\r\n                            <th class=\"text-center\" width=\"10%\">QUANTITY</th>\r\n                            <th class=\"text-right\" width=\"20%\">LINE TOTAL</th>\r\n                          </tr>\r\n                        </thead>\r\n                        <tbody>\r\n                          <tr v-for=\"item in productdata.cart\" :key=\"item.name\">\r\n                            <td>\r\n                              <span class=\"text-inverse\">{{ item.name }}</span\r\n                              ><br />\r\n                              <small>{{ item.brand }}</small>\r\n                            </td>\r\n                            <td class=\"text-center\">{{ item.price }} DT</td>\r\n                            <td class=\"text-center\">{{ item.quantity }}</td>\r\n                            <td class=\"text-right\">{{ item.totalPrice }} DT</td>\r\n                          </tr>\r\n                        </tbody>\r\n                      </table>\r\n                    </div>\r\n                    <!-- end table-responsive -->\r\n                    <!-- begin invoice-price -->\r\n                    <div class=\"invoice-price\">\r\n                      <div class=\"invoice-price-left\">\r\n                        <div class=\"invoice-price-row\">\r\n                          <div class=\"sub-price\">\r\n                            <small>SUBTOTAL</small>\r\n                            <span class=\"text-inverse\"\r\n                              >{{ productdata.total - 7 }} DT</span\r\n                            >\r\n                          </div>\r\n                          <div class=\"sub-price\">\r\n                            <i class=\"fa fa-plus text-muted\"></i>\r\n                          </div>\r\n                          <div class=\"sub-price\">\r\n                            <small>SHIPPING</small>\r\n                            <span class=\"text-inverse\">7 DT</span>\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                      <div class=\"invoice-price-right\">\r\n                        <small>TOTAL</small>\r\n                        <span class=\"f-w-600\">{{ productdata.total }} DT</span>\r\n                      </div>\r\n                    </div>\r\n                    <!-- end invoice-price -->\r\n                  </div>\r\n                  <!-- end invoice-content -->\r\n                  <!-- begin invoice-note -->\r\n                  <div class=\"invoice-note\">\r\n                    * Make all cheques payable to SelDig<br />\r\n                    * Payment is due within 30 days<br />\r\n                    * If you have any questions concerning this invoice, contact\r\n                    contact@rani4tech.com\r\n                  </div>\r\n                  <!-- end invoice-note -->\r\n                  <!-- begin invoice-footer -->\r\n                  <div class=\"invoice-footer\">\r\n                    <p class=\"text-center m-b-5 f-w-600\">\r\n                      THANK YOU FOR YOUR BUSINESS\r\n                    </p>\r\n                    <p class=\"text-center\">\r\n                      <span class=\"m-r-10\"\r\n                        ><i class=\"fa fa-fw fa-lg fa-globe\"></i>\r\n                        www.rani4tech.com</span\r\n                      >\r\n                      <span class=\"m-r-10\"\r\n                        ><i class=\"fa fa-fw fa-lg fa-phone-volume\"></i>\r\n                        T:216-99548790</span\r\n                      >\r\n                      <span class=\"m-r-10\"\r\n                        ><i class=\"fa fa-fw fa-lg fa-envelope\"></i>\r\n                        ranizouaouicontact@gmail.com</span\r\n                      >\r\n                    </p>\r\n                  </div>\r\n                  <!-- end invoice-footer -->\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <!-- form end here -->\r\n          </div>\r\n          <div class=\"modal-footer\">\r\n            <button\r\n              type=\"button\"\r\n              id=\"close\"\r\n              class=\"btn btn-secondary\"\r\n              data-bs-dismiss=\"modal\"\r\n            >\r\n              Close\r\n            </button>\r\n            <button\r\n              type=\"button\"\r\n              class=\"btn\"\r\n              style=\"background: red; color: white\"\r\n              @click=\"printinvoice\"\r\n            >\r\n              Export as PDF\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- fin model -->\r\n  </div>\r\n</template>\r\n<script>\r\nimport \"../Firebase\";\r\n\r\nimport {\r\n  collection,\r\n  query,\r\n  getFirestore,\r\n  onSnapshot,\r\n  doc,\r\n  deleteDoc,\r\n} from \"firebase/firestore\";\r\nimport printJS from \"print-js\";\r\nconst db = getFirestore();\r\nconst q = query(collection(db, \"Checkout\"));\r\n\r\nexport default {\r\n  name: \"OrdersView\",\r\n  data() {\r\n    return {\r\n      orders: [],\r\n      dataProduct: {\r\n        name: \"\",\r\n        price: 0,\r\n        image: \"\",\r\n        brand: \"\",\r\n        description: \"\",\r\n        key: \"\",\r\n      },\r\n      productdata: {\r\n        address: \"\",\r\n        date: \"\",\r\n        name: \"\",\r\n        number: \"\",\r\n        total: \"\",\r\n        cart: [],\r\n        key: \"\",\r\n      },\r\n    };\r\n  },\r\n  created() {\r\n    onSnapshot(q, (querySnapshot) => {\r\n      this.orders = [];\r\n      querySnapshot.forEach((doc) => {\r\n        this.orders.push({\r\n          key: doc.id,\r\n          name: doc.data().name,\r\n          address: doc.data().address,\r\n          number: doc.data().number,\r\n          date: doc.data().date,\r\n          total: doc.data().total,\r\n          cart: doc.data().cart,\r\n        });\r\n      });\r\n    });\r\n  },\r\n  methods: {\r\n    getDataKey(key, name) {\r\n      this.dataProduct.key = key;\r\n      this.dataProduct.name = name;\r\n    },\r\n\r\n    RemoveFromCheckout(key) {\r\n      deleteDoc(doc(db, \"Checkout\", key)).then(() => {\r\n        document.getElementById(\"close\").click();\r\n      });\r\n    },\r\n    GetOrderData(name, date, address, number, cart, key, total) {\r\n      this.productdata.name = name;\r\n      this.productdata.date = date;\r\n      this.productdata.address = address;\r\n      this.productdata.number = number;\r\n      this.productdata.cart = cart;\r\n      this.productdata.key = key;\r\n      this.productdata.total = total;\r\n      console.log(this.productdata);\r\n    },\r\n    printinvoice() {\r\n      printJS({\r\n        printable: \"printJS-form\",\r\n        type: \"html\",\r\n        targetStyles: [\"*\"],\r\n        scanStyles: true,\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\nbody {\r\n  background: rgb(255, 255, 255);\r\n}\r\n\r\n.invoice {\r\n  background: #fff;\r\n  padding: 20px;\r\n}\r\n\r\n.invoice-company {\r\n  font-size: 20px;\r\n}\r\n\r\n.invoice-header {\r\n  margin: 0 -20px;\r\n  background: #f0f3f4;\r\n  padding: 20px;\r\n}\r\n\r\n.invoice-date,\r\n.invoice-from,\r\n.invoice-to {\r\n  display: table-cell;\r\n  width: 1%;\r\n}\r\n\r\n.invoice-from,\r\n.invoice-to {\r\n  padding-right: 20px;\r\n}\r\n\r\n.invoice-date .date,\r\n.invoice-from strong,\r\n.invoice-to strong {\r\n  font-size: 16px;\r\n  font-weight: 600;\r\n}\r\n\r\n.invoice-date {\r\n  text-align: right;\r\n  padding-left: 20px;\r\n}\r\n\r\n.invoice-price {\r\n  background: #f0f3f4;\r\n  display: table;\r\n  width: 100%;\r\n}\r\n\r\n.invoice-price .invoice-price-left,\r\n.invoice-price .invoice-price-right {\r\n  display: table-cell;\r\n  padding: 20px;\r\n  font-size: 20px;\r\n  font-weight: 600;\r\n  width: 75%;\r\n  position: relative;\r\n  vertical-align: middle;\r\n}\r\n\r\n.invoice-price .invoice-price-left .sub-price {\r\n  display: table-cell;\r\n  vertical-align: middle;\r\n  padding: 0 20px;\r\n}\r\n\r\n.invoice-price small {\r\n  font-size: 12px;\r\n  font-weight: 400;\r\n  display: block;\r\n}\r\n\r\n.invoice-price .invoice-price-row {\r\n  display: table;\r\n  float: left;\r\n}\r\n\r\n.invoice-price .invoice-price-right {\r\n  width: 25%;\r\n  background: #2d353c;\r\n  color: #fff;\r\n  font-size: 28px;\r\n  text-align: right;\r\n  vertical-align: bottom;\r\n  font-weight: 300;\r\n}\r\n\r\n.invoice-price .invoice-price-right small {\r\n  display: block;\r\n  opacity: 0.6;\r\n  position: absolute;\r\n  top: 10px;\r\n  left: 10px;\r\n  font-size: 12px;\r\n}\r\n\r\n.invoice-footer {\r\n  border-top: 1px solid #ddd;\r\n  padding-top: 10px;\r\n  font-size: 10px;\r\n}\r\n\r\n.invoice-note {\r\n  color: #999;\r\n  margin-top: 80px;\r\n  font-size: 85%;\r\n}\r\n\r\n.invoice > div:not(.invoice-footer) {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.btn.btn-white,\r\n.btn.btn-white.disabled,\r\n.btn.btn-white.disabled:focus,\r\n.btn.btn-white.disabled:hover,\r\n.btn.btn-white[disabled],\r\n.btn.btn-white[disabled]:focus,\r\n.btn.btn-white[disabled]:hover {\r\n  color: #2d353c;\r\n  background: #fff;\r\n  border-color: #d9dfe3;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}