{"ast":null,"code":"// @ is an alias to /src\nimport \"../Firebase\";\nimport { getAuth, createUserWithEmailAndPassword } from \"firebase/auth\";\nimport { getFirestore, collection, addDoc } from \"firebase/firestore\";\nconst auth = getAuth();\nconst db = getFirestore();\nconst dbRef = collection(db, \"users\");\nimport router from \"../router\";\nexport default {\n  name: \"SignUpView\",\n\n  data() {\n    return {\n      user: {\n        name: \"\",\n        email: \"\",\n        address: \"\",\n        password: \"\",\n        number: \"\",\n        uid: \"\"\n      }\n    };\n  },\n\n  methods: {\n    signupUser() {\n      createUserWithEmailAndPassword(auth, this.user.email, this.user.password).then(dataUser => {\n        addDoc(dbRef, {\n          name: this.user.name,\n          email: this.user.email,\n          address: this.user.address,\n          number: this.user.number,\n          uid: dataUser.user.uid\n        });\n        localStorage.setItem(\"uidUser\", dataUser.user.uid);\n\n        if (this.$store.state.count > 0) {\n          router.push(\"/cart\");\n        } else {\n          router.push(\"/products\");\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsDA;AACA,OAAO,aAAP;AACA,SAASA,OAAT,EAAkBC,8BAAlB,QAAwD,eAAxD;AACA,SAASC,YAAT,EAAuBC,UAAvB,EAAmCC,MAAnC,QAAiD,oBAAjD;AACA,MAAMC,IAAG,GAAIL,OAAO,EAApB;AACA,MAAMM,EAAC,GAAIJ,YAAY,EAAvB;AACA,MAAMK,KAAI,GAAIJ,UAAU,CAACG,EAAD,EAAK,OAAL,CAAxB;AACA,OAAOE,MAAP,MAAmB,WAAnB;AACA,eAAe;EACbC,IAAI,EAAE,YADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJF,IAAI,EAAE,EADF;QAEJG,KAAK,EAAE,EAFH;QAGJC,OAAO,EAAE,EAHL;QAIJC,QAAQ,EAAE,EAJN;QAKJC,MAAM,EAAE,EALJ;QAMJC,GAAG,EAAE;MAND;IADD,CAAP;EAUD,CAbY;;EAcbC,OAAO,EAAE;IACPC,UAAU,GAAG;MACXjB,8BAA8B,CAACI,IAAD,EAAO,KAAKM,IAAL,CAAUC,KAAjB,EAAwB,KAAKD,IAAL,CAAUG,QAAlC,CAA9B,CACGK,IADH,CACSC,QAAD,IAAc;QAClBhB,MAAM,CAACG,KAAD,EAAQ;UACZE,IAAI,EAAE,KAAKE,IAAL,CAAUF,IADJ;UAEZG,KAAK,EAAE,KAAKD,IAAL,CAAUC,KAFL;UAGZC,OAAO,EAAE,KAAKF,IAAL,CAAUE,OAHP;UAIZE,MAAM,EAAE,KAAKJ,IAAL,CAAUI,MAJN;UAKZC,GAAG,EAAEI,QAAQ,CAACT,IAAT,CAAcK;QALP,CAAR,CAAN;QAOAK,YAAY,CAACC,OAAb,CAAqB,SAArB,EAAgCF,QAAQ,CAACT,IAAT,CAAcK,GAA9C;;QACA,IAAI,KAAKO,MAAL,CAAYC,KAAZ,CAAkBC,KAAlB,GAA0B,CAA9B,EAAiC;UAC/BjB,MAAM,CAACkB,IAAP,CAAY,OAAZ;QACF,CAFA,MAEO;UACLlB,MAAM,CAACkB,IAAP,CAAY,WAAZ;QACF;MACD,CAfH,EAgBGC,KAhBH,CAgBUC,KAAD,IAAW;QAChBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACD,CAlBH;IAmBD;;EArBM;AAdI,CAAf","names":["getAuth","createUserWithEmailAndPassword","getFirestore","collection","addDoc","auth","db","dbRef","router","name","data","user","email","address","password","number","uid","methods","signupUser","then","dataUser","localStorage","setItem","$store","state","count","push","catch","error","console","log"],"sourceRoot":"","sources":["C:\\Users\\muham\\OneDrive\\Masaüstü\\project\\Online-Store\\src\\views\\SignUpView.vue"],"sourcesContent":["<template>\r\n  <div class=\"vh-100 d-flex justify-content-center align-items-center\" style=\"background: rgb(57 181 255)\">\r\n    <div class=\"container\">\r\n      <div class=\"row d-flex justify-content-center\">\r\n        <div class=\"col-12 col-md-8 col-lg-6\">\r\n          <div class=\"card bg-white\">\r\n            <div class=\"card-body p-5\">\r\n              <form class=\"mb-3 mt-md-4\" @submit.prevent=\"signupUser\">\r\n                <h2 class=\"fw-bold mb-2 text-uppercase\" style=\"margin-left: 116px\">\r\n                  Join Us !\r\n                </h2>\r\n                <!-- <p class=\"mb-5\">Please enter your login and password!</p> -->\r\n                <div class=\"mb-3\">\r\n                  <label for=\"exampleInputEmail1\" class=\"form-label\">Name</label>\r\n                  <input type=\"name\" class=\"form-control\" aria-describedby=\"emailHelp\" placeholder=\"Your Name\"\r\n                    v-model=\"user.name\" />\r\n                </div>\r\n                <div class=\"mb-3\">\r\n                  <label for=\"email\" class=\"form-label\">Email address</label>\r\n                  <input type=\"email\" class=\"form-control\" id=\"email\" placeholder=\"name@example.com\"\r\n                    v-model=\"user.email\" />\r\n                </div>\r\n                <div class=\"mb-3\">\r\n                  <label for=\"password\" class=\"form-label\">Password</label>\r\n                  <input type=\"password\" class=\"form-control\" id=\"password\" placeholder=\"*******\"\r\n                    v-model=\"user.password\" />\r\n                </div>\r\n                <div class=\"mb-3\">\r\n                  <label for=\"adress\" class=\"form-label\">Shipping Address</label>\r\n                  <input type=\"address\" class=\"form-control\" aria-describedby=\"emailHelp\"\r\n                    placeholder=\"ex: Ghardimaou, Jendouba, 8160\" v-model=\"user.address\" />\r\n                </div>\r\n                <div class=\"form-outline\">\r\n                  <label class=\"form-label\" for=\"typePhone\">Phone number</label>\r\n                  <input type=\"tel\" id=\"typePhone\" class=\"form-control\" placeholder=\"ex: 97 384 764\"\r\n                    v-model=\"user.number\" />\r\n                </div>\r\n\r\n                <div class=\"d-grid\">\r\n                  <br />\r\n                  <button class=\"btn btn-outline-dark\" type=\"submit\">\r\n                    Login\r\n                  </button>\r\n                </div>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\nimport \"../Firebase\";\r\nimport { getAuth, createUserWithEmailAndPassword } from \"firebase/auth\";\r\nimport { getFirestore, collection, addDoc } from \"firebase/firestore\";\r\nconst auth = getAuth();\r\nconst db = getFirestore();\r\nconst dbRef = collection(db, \"users\");\r\nimport router from \"../router\";\r\nexport default {\r\n  name: \"SignUpView\",\r\n  data() {\r\n    return {\r\n      user: {\r\n        name: \"\",\r\n        email: \"\",\r\n        address: \"\",\r\n        password: \"\",\r\n        number: \"\",\r\n        uid: \"\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    signupUser() {\r\n      createUserWithEmailAndPassword(auth, this.user.email, this.user.password)\r\n        .then((dataUser) => {\r\n          addDoc(dbRef, {\r\n            name: this.user.name,\r\n            email: this.user.email,\r\n            address: this.user.address,\r\n            number: this.user.number,\r\n            uid: dataUser.user.uid,\r\n          });\r\n          localStorage.setItem(\"uidUser\", dataUser.user.uid);\r\n          if (this.$store.state.count > 0) {\r\n            router.push(\"/cart\");\r\n          } else {\r\n            router.push(\"/products\");\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.mt-md-4 {\r\n  margin-top: -0.5rem !important;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}