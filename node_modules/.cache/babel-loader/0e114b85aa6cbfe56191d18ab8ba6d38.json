{"ast":null,"code":"import \"../Firebase\";\nimport { getAuth, signOut, onAuthStateChanged } from \"firebase/auth\";\nimport router from \"../router\";\nconst auth = getAuth();\nexport default {\n  name: \"NavbarView\",\n\n  data() {\n    return {\n      CheckAuth: null\n    };\n  },\n\n  methods: {\n    logout() {\n      signOut(auth).then(() => {\n        localStorage.removeItem(\"uidUser\");\n        router.push(\"/\");\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n  },\n\n  created() {\n    onAuthStateChanged(auth, user => {\n      if (user) {\n        this.CheckAuth = user;\n      } else {\n        this.CheckAuth = null;\n      }\n    });\n  },\n\n  computed: {\n    count() {\n      return this.$store.state.count;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkFA,OAAO,aAAP;AACA,SAASA,OAAT,EAAkBC,OAAlB,EAA2BC,kBAA3B,QAAqD,eAArD;AACA,OAAOC,MAAP,MAAmB,WAAnB;AACA,MAAMC,IAAG,GAAIJ,OAAO,EAApB;AACA,eAAe;EACbK,IAAI,EAAE,YADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MAAEC,SAAS,EAAE;IAAb,CAAP;EACD,CAJY;;EAKbC,OAAO,EAAE;IACPC,MAAM,GAAG;MACPR,OAAO,CAACG,IAAD,CAAP,CACGM,IADH,CACQ,MAAM;QACVC,YAAY,CAACC,UAAb,CAAwB,SAAxB;QACAT,MAAM,CAACU,IAAP,CAAY,GAAZ;MACD,CAJH,EAKGC,KALH,CAKUC,KAAD,IAAW;QAChBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACD,CAPH;IAQD;;EAVM,CALI;;EAiBbG,OAAO,GAAG;IACRhB,kBAAkB,CAACE,IAAD,EAAQe,IAAD,IAAU;MACjC,IAAIA,IAAJ,EAAU;QACR,KAAKZ,SAAL,GAAiBY,IAAjB;MACF,CAFA,MAEO;QACL,KAAKZ,SAAL,GAAiB,IAAjB;MACF;IACD,CANiB,CAAlB;EAOD,CAzBY;;EA0Bba,QAAQ,EAAE;IACRC,KAAK,GAAG;MACN,OAAO,KAAKC,MAAL,CAAYC,KAAZ,CAAkBF,KAAzB;IACD;;EAHO;AA1BG,CAAf","names":["getAuth","signOut","onAuthStateChanged","router","auth","name","data","CheckAuth","methods","logout","then","localStorage","removeItem","push","catch","error","console","log","created","user","computed","count","$store","state"],"sourceRoot":"","sources":["C:\\Users\\muham\\OneDrive\\Masaüstü\\project\\Online-Store\\src\\views\\NavbarView.vue"],"sourcesContent":["<template>\r\n  <nav class=\"navbar navbar-expand-lg\" style=\"background: #39b5ff\">\r\n    <div class=\"container\">\r\n      <button\r\n        class=\"navbar-toggler\"\r\n        type=\"button\"\r\n        data-bs-toggle=\"collapse\"\r\n        data-bs-target=\"#navbarNav\"\r\n        aria-controls=\"navbarTogglerDemo02\"\r\n        aria-expanded=\"false\"\r\n        aria-label=\"Toggle navigation\"\r\n      >\r\n        <span class=\"navbar-toggler-icon\"></span>\r\n      </button>\r\n      <router-link to=\"/\"\r\n        ><img src=\"https://static.ticimax.cloud/52168/Uploads/EditorUploads/suud-image/logo.svg\" width=\"120\" height=\"40\"\r\n      /></router-link>\r\n      <div class=\"container-fluid\">\r\n        <div class=\"collapse navbar-collapse\" id=\"navbarNav\">\r\n          <ul\r\n            class=\"navbar-nav me-auto my-2 my-lg-0 navbar-nav-scroll\"\r\n            style=\"--bs-scroll-height: 100px\"\r\n          >\r\n            <li class=\"nav-item\">\r\n              <router-link\r\n                class=\"nav-link active\"\r\n                style=\"color: white\"\r\n                aria-current=\"page\"\r\n                to=\"/\"\r\n                >Ana Sayfa</router-link\r\n              >\r\n            </li>\r\n            <li class=\"nav-item\">\r\n              <router-link class=\"nav-link\" to=\"/products\"\r\n                >Ürünler</router-link\r\n              >\r\n            </li>\r\n            <li class=\"nav-item\">\r\n              <router-link class=\"nav-link\" v-if=\"CheckAuth\" to=\"/myproducts\"\r\n                >Urunlerim</router-link\r\n              >\r\n            </li>\r\n\r\n            <li class=\"nav-item\">\r\n              <router-link class=\"nav-link\" v-if=\"CheckAuth == null\" to=\"/login\"\r\n                >Giriş Yap</router-link\r\n              >\r\n            </li>\r\n            <li class=\"nav-item\">\r\n              <router-link class=\"nav-link\" v-if=\"CheckAuth\" to=\"/orders\"\r\n                >Siparişler</router-link\r\n              >\r\n            </li>\r\n            <li class=\"nav-item\">\r\n              <a\r\n                class=\"nav-link\"\r\n                style=\"cursor: pointer\"\r\n                v-if=\"CheckAuth\"\r\n                v-on:click=\"logout\"\r\n                >Logout</a\r\n              >\r\n            </li>\r\n            <li>\r\n              <router-link\r\n                id=\"cart\"\r\n                to=\"/cart\"\r\n                class=\"cart\"\r\n                :data-totalitems=\"count\"\r\n              >\r\n                <i class=\"fas fa-shopping-cart\"></i>\r\n              </router-link>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </nav>\r\n\r\n  <router-view />\r\n</template>\r\n\r\n<script>\r\nimport \"../Firebase\";\r\nimport { getAuth, signOut, onAuthStateChanged } from \"firebase/auth\";\r\nimport router from \"../router\";\r\nconst auth = getAuth();\r\nexport default {\r\n  name: \"NavbarView\",\r\n  data() {\r\n    return { CheckAuth: null };\r\n  },\r\n  methods: {\r\n    logout() {\r\n      signOut(auth)\r\n        .then(() => {\r\n          localStorage.removeItem(\"uidUser\");\r\n          router.push(\"/\");\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n  },\r\n  created() {\r\n    onAuthStateChanged(auth, (user) => {\r\n      if (user) {\r\n        this.CheckAuth = user;\r\n      } else {\r\n        this.CheckAuth = null;\r\n      }\r\n    });\r\n  },\r\n  computed: {\r\n    count() {\r\n      return this.$store.state.count;\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.cart {\r\n  position: fixed;\r\n  top: 20px;\r\n  right: 40px;\r\n  width: 30px;\r\n  height: 30px;\r\n  background: #292d48;\r\n  display: -webkit-box;\r\n  display: -ms-flexbox;\r\n  display: flex;\r\n  -webkit-box-align: center;\r\n  -ms-flex-align: center;\r\n  align-items: center;\r\n  -webkit-box-pack: center;\r\n  -ms-flex-pack: center;\r\n  justify-content: center;\r\n  border-radius: 5px;\r\n}\r\n\r\n.cart i {\r\n  font-size: 25px;\r\n  color: white;\r\n}\r\n\r\n.cart:before {\r\n  content: attr(data-totalitems);\r\n  font-size: 12px;\r\n  font-weight: 600;\r\n  position: absolute;\r\n  top: -12px;\r\n  right: -12px;\r\n  background: #ff0000;\r\n  line-height: 24px;\r\n  padding: 0 5px;\r\n  height: 24px;\r\n  min-width: 24px;\r\n  color: white;\r\n  text-align: center;\r\n  border-radius: 24px;\r\n}\r\n\r\n.cart.shake {\r\n  -webkit-animation: shakeCart 0.4s ease-in-out forwards;\r\n  animation: shakeCart 0.4s ease-in-out forwards;\r\n}\r\n.navbar-expand-lg .navbar-nav .nav-link {\r\n  color: white;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}